<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/app_navigation"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.partners.texsun.app.app.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_pendingVerificationFragment"
            app:destination="@id/pendingVerificationFragment" />
    </fragment>
    <fragment
        android:id="@+id/pendingVerificationFragment"

        android:name="com.partners.texsun.app.boarding.PendingVerificationFragment"
        android:label="fragment_pending_verification"
        tools:layout="@layout/fragment_pending_verification" />
    <fragment
        android:id="@+id/configurationMenuFragment"

        android:name="com.partners.texsun.app.app.configuration.ConfigurationMenuFragment"
        android:label="fragment_configuration_menu"
        tools:layout="@layout/fragment_configuration_menu" />
</navigation>